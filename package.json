{
  "name": "bitgesell-wallet-js-frontend-core",
  "version": "0.4.2",
  "description": "Bitgesell Wallet JS Frontend Core",
  "main": "local-server.js",
  "author": "epexa",
  "license": "Proprietary",
  "config": {
    "IP": "192.168.1.254",
    "PORT": "80",
    "DIST_FOLDER": "../bitgesell-wallet-js-frontend-core-dist"
  },
