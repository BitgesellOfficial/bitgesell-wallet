    "clean-css-cli": "^4.2.1",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "html-minifier": "^3.5.21",
    "husky": "^2.3.0",
    "uglify-js": "^3.5.2"
  }
